{
    "name": "devjaythakkar/money-wrapper",
    "description": "MoneyPHP Wrapper",
    "keywords": ["money", "php", "wrapper", "package", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Thakkar",
            "email": "jaythakkar381@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JayThakkar\\MoneyWrapper\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JayThakkar\\MoneyWrapper\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^9.0|^10.0",
        "moneyphp/money": "^4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "8.0",
        "mockery/mockery": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JayThakkar\\MoneyWrapper\\MoneyWrapperServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
